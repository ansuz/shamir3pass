    "maxstatements": 12,
    "noarg": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": true,
    "strict": "implied",
    "predef": [
        "console",
        "define",
        "require",
        "module",
        "__dirname"
    ],
    "globals": {
        "self": true,
        "setTimeout": true,
        "clearTimeout": true,
        "setInterval": true,
        "clearInterval": true,
        "Buffer": true,
        "Uint8Array": true
    }
