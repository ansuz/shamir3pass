{
    "bitwise": true,
    "esversion": 5,
    "laxcomma": true,
    "laxbreak": true,
    "sub": true,
    "curly": true,
    "eqeqeq": true,
    "iterator": true,
    "latedef": true,
    "nocomma": true,
    "notypeof": true,
    "shadow": false,
    "undef": true,
    "unused": true,
    "futurehostile":true,
    "eqeqeq": true,
    "freeze": true,
    "funcscope": true,
    "iterator": true,
    "maxcomplexity": 7,
    "maxdepth": 2,
